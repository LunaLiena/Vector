{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "./src/renderer/src",
    "paths": {
      "@renderer/*":["./*"],
      "@api/*":["api/*"],
      "@types/*":["types/*"],
      "@store/*": ["store/*"],
      "@components/*": ["components/*"],
      "@shared/*":["components/roles/shared/*"],
      "@api-types/*":["api/apiTypes/*"],
      "@services/*":["services/*"],
      "@role-components/*":["components/roles/*"],
      "@styles/*":["style/*"],
      "@hooks/*":["hooks/*"],
    }
  }
}
